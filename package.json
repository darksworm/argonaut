{
  "name": "argonaut-cli",
  "author": "darksworm (Ilmars Janis Bluzmanis)",
  "version": "1.2.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "tsx src/main.tsx",
    "start": "node dist/index.js",
    "build": "rollup -c",
    "build:sea": "rollup -c rollup.config.sea.js",
    "sea:prep": "node --experimental-sea-config sea-config.json",
    "sea:build": "npm run build:sea && npm run sea:prep",
    "sea:executable": "node scripts/build-sea.js",
    "prepublishOnly": "npm run build"
  },
  "description": "A CLI tool for managing and interacting with Argo CD.",
  "engines": {
    "node": ">=18"
  },
  "bugs": {
    "url": "https://github.com/darksworm/argonaut/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darksworm/argonaut.git"
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "chalk": "^5.5.0",
    "execa": "^9.6.0",
    "ink": "^6.1.0",
    "ink-text-input": "^6.0.0",
    "node-pty": "^1.0.0",
    "react": "^19.1.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "postject": "^1.0.0-alpha.6",
    "rollup": "^4.46.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "bin": {
    "argonaut": "./dist/cli.js"
  },
  "keywords": [
    "argocd",
    "argo-cd",
    "k8s",
    "kubernetes",
    "gitops",
    "continuous-delivery",
    "cd",
    "devops",
    "infrastructure-as-code",
    "applications",
    "applicationset",
    "resource-tree",
    "sync",
    "rollback",
    "health",
    "status",
    "events",
    "projects",
    "clusters",
    "repositories",
    "helm",
    "kustomize",
    "k9s-like",
    "terminal-ui",
    "text-ui",
    "react-cli",
    "react-tui",
    "tui",
    "terminal",
    "cli",
    "yaml"
  ]
}
