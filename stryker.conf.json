{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "testRunner": "command",
  "commandRunner": {
    "command": "bun test"
  },
  "mutate": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "!src/__tests__/**"
  ],
  "coverageAnalysis": "off",
  "reporters": ["html", "clear-text", "progress", "dashboard"],
  "htmlReporter": {
    "fileName": "reports/mutation/html/index.html"
  },
  "dashboard": {
    "project": "github.com/darksworm/argonaut",
    "version": "main",
    "module": "command-testing"
  },
  "timeoutMS": 60000,
  "concurrency": 18,
  "checkers": ["typescript"],
  "tsconfigFile": "tsconfig.json",
  "tempDirName": "stryker-tmp"
}
